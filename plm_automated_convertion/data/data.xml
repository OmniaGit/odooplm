<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="odoo_local_server" model="plm.convert.servers">
            <field name="sequence">1</field>
            <field name="name">Odoo Internal Convertion Server</field>
            <field name="is_internal">True</field>
            <field name="available">True</field>
        </record>
        <record id="update_dxf_preview_pdf" model="plm.convert.format">
        	<field name="server_id" ref="odoo_local_server"/>
        	<field name="available">True</field>
            <field name="cad_name">zedxf</field>
            <field name="start_format">.dxf</field>
            <field name="end_format">.pdf</field>
        </record>
       <record id="update_dxf_preview_png" model="plm.convert.format">
        	<field name="server_id" ref="odoo_local_server"/>
        	<field name="available">True</field>
            <field name="cad_name">zedxf</field>
            <field name="start_format">.dxf</field>
            <field name="end_format">.png</field>
        </record>
       <record id="update_dxf_preview_svg" model="plm.convert.format">
        	<field name="server_id" ref="odoo_local_server"/>
        	<field name="available">True</field>
            <field name="cad_name">zedxf</field>
            <field name="start_format">.dxf</field>
            <field name="end_format">.svg</field>
        </record>
    </data>
</odoo>